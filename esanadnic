    <!-- Core JavaScript Files -->
    <script src="js/jquery-1.10.2.js"></script>
    <script src="bootstrap/js/bootstrap.min.js"></script>
    <script src="js/jquery.easing.min.js"></script>
    <script src="js/wow.min.js"></script>
    <script src="js/jquery.scrollTo.js"></script>
    <script src="js/custom.js"></script>
    <script src="bootstrap-validator/bootstrapValidator.js"></script>

    <script type="text/javascript">
        $(document).ready(function() {
            //alert(window.navigator.userAgent);
            // Setting the bootstrap validations
            $("#eregisterForm")
                .bootstrapValidator({
                    framework: 'bootstrap',
                    feedbackIcons: {
                        valid: 'glyphicon glyphicon-ok',
                        invalid: 'glyphicon glyphicon-remove',
                        validating: 'glyphicon glyphicon-refresh'
                    },
                    fields: {
                        documentID: {
                            validators: {
                                regexp: {
                                    regexp: /^[0-9\s]+$/,
                                    message: 'Only numeric value is allowed.'
                                },
                                notEmpty: {
                                    message: 'Document ID is required.'
                                }
                            }
                        },
                        captcha: {
                            validators: {
                                notEmpty: {
                                    message: 'Captcha is required.'
                                },
                                regexp: {
                                    regexp: /^[A-Z0-9]+$/,
                                    message: 'Invalid Captcha.'
                                }
                            }
                        }
                    }
                });

            $('#refreshCaptcha').on('click', function(e) {
                //alert('captcha');
                $.ajax({
                    type: "get",
                    url: "Captcha.jpg", //this is my servlet                  
                    success: function(data) {
                        $(".captach").html(' <img src="Captcha.jpg?' + Math.random() + '" style="width: 125px;height: 38px;"/>');
                    }
                });
            });

        });
    </script>

